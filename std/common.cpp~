#include "common.hpp"

template<class T> void swap(T &start, T &end)
{
	if(start != end)
	{
		T tmp = end;
		end = start;
		start = tmp;
	}
}

template<class T> void swap(T *start, T* end)
{
	if(*start != *end)
	{
		T tmp = *end;
		*end = *start;
		*start = tmp;
	}
}




